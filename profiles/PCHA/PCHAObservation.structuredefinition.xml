  <description value="PCHA observation profile. Adds a way to reference both a SensorDevice and a PHGDevice from the Observation." />
  <kind value="resource" />
  <constrainedType value="Observation" />
  <abstract value="false" />
  <base value="http://hl7.org/fhir/StructureDefinition/Observation" />
  <differential>
    <element>
      <path value="Observation" />
    </element>
    <element>
      <path value="Observation.device" />
      <slicing>
        <description value="One Reference for the Sensor Device and another reference for the PHG Device" />
        <rules value="openAtEnd" />
      </slicing>
    </element>
    <element>
      <path value="Observation.device" />
      <name value="PHG" />
      <short value="PHG reference" />
