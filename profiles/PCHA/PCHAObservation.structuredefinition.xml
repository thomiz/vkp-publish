      <type>
        <code value="Reference" />
        <profile value="http://hl7.org/fhir/StructureDefinition/Device" />
        <aggregation value="referenced" />
      </type>
    </element>
    <element>
      <path value="Observation.device" />
      <name value="SensorDevice" />
      <short value="Sensor reference" />
      <definition value="Reference to the SensorDevice?" />
      <type>
        <code value="Reference" />
        <profile value="http://hl7.org/fhir/StructureDefinition/Device" />
        <aggregation value="referenced" />
      </type>
    </element>
  </differential>
